\gdef\@volume{0}

\def\pubyear#1{\gdef\@pubyear{#1}}
\gdef\@pubyear{\year}

\def\firstpage#1{\gdef\@firstpage{#1} \c@page=0 \global\advance\c@page by #1}
\gdef\@firstpage{1}

\newcount\@lastpage
\def\lastpage#1{\@lastpage=#1}
\def\setlastpage{\@ifundefined{r@lastpage}{}#
{\expandafter\@setlastp\r@lastpage}}
\def\@setlastp#1#2{\@lastpage=#2}
\@lastpage=1

\def\received#1{\gdef\@pubdate{(#1)}}
\gdef\@pubdate{\@date}



\def\subequation{\refstepcounter{equation}%
  \begingroup
  \edef\old@equation{\the\c@equation}%
  \edef\old@theequation{\theequation}%
  \setcounter{equation}{0}%
  \def\theequation{\old@theequation\alph{equation}}}

\def\endsubequation{%
  \setcounter{equation}{\old@equation}%
  \endgroup
  \global\@ignoretrue}



